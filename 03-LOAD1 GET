SETM O214,1
GOTO *END
ENDIF
*211
RINT I197,0,0,0,0
MOVEX A=1,AC=0,SM=0,M1X,L,P11,R=V31%,H=1,MS	'P11
INH
LETVI V19%,211
IF I197=1
GETANGLE V90!
MOVEX A=1,AC=0,SM=0,M1J,L,(V90!,V91!,V92!,V93!,V94!,V95!),S= 150.0,H=1,MS
SETM O214,1
GOTO *END
ENDIF
*9
RINT I197,0,0,0,0
MOVEX A=1,AC=0,SM=0,M1X,L,P9,R=V31%,H=1,MS	'P9
INH
LETVI V19%,9
IF I197=1
GETANGLE V90!
MOVEX A=1,AC=0,SM=0,M1J,L,(V90!,V91!,V92!,V93!,V94!,V95!),S= 150.0,H=1,MS
